{
  "kind": "collectionType",
  "collectionName": "projects",
  "info": {
    "singularName": "project",
    "pluralName": "projects",
    "displayName": "Project",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "ProjectTitle": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "Status": {
      "type": "enumeration",
      "enum": [
        "-1. אוכלס",
        "-2. בביצוע ",
        "-3. היתר בנייה ",
        "-4. היתר בתנאים "
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "IsTheProjectBeingMarketed": {
      "type": "boolean",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ProjectType": {
      "type": "enumeration",
      "enum": [
        "- 1. תמ\"א - הריסה ובניה",
        "-2. תמ\"א - עיבוי וחיזוק",
        "-3. יזמות קלאסית",
        "-4. פינוי בינוי"
      ],
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ApartmentsAmount": {
      "type": "integer",
      "required": true,
      "min": 0,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ArchitectName": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ImagesForCoverGallery": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "WideMainSimulationImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "SquareMainSimulationImage": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ProjectPageTitle": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ProjectPageParagraph": {
      "type": "string",
      "required": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ProjectPageButtonWideImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ],
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "ProjectSliderIconImageUpload": {
      "type": "component",
      "repeatable": true,
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "icon-for-the-slider.project-slider-icon-image-upload"
    },
    "project_s_apartments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::projects-apartment.projects-apartment",
      "mappedBy": "project"
